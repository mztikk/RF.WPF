<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:RF.WPF.MVVM.Converters"
    >
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/Colors.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/RF.Values.xaml"/>

        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/GraphicsDictionary.xaml"/>

        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/GeneralStyles.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/ButtonStyles.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/TabHeader.xaml"/>
        <ResourceDictionary Source="pack://application:,,,/RF.WPF;component/Resources/Window.xaml"/>

    </ResourceDictionary.MergedDictionaries>

    <converters:InverseBooleanConverter x:Key="InverseBooleanConverter"/>
    <converters:StringLengthVisibleConverter x:Key="StringLengthVisibleConverter"/>
    <converters:BoolVisibilityConverter x:Key="BoolVisibilityConverter"/>
</ResourceDictionary>

